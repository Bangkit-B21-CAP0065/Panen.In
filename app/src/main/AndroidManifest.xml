<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.panenin.bangkit.b21.cap0065">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_panenin_polos"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_panenin_polos"
        android:supportsRtl="true"
        android:theme="@style/Theme.PanenIn">
        <activity android:name=".WelcomeActivity"/>
        <activity android:name=".SplashActivity"
            android:theme="@style/Theme.PanenIn.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.home.MarketVisualizationActivity" />
        <activity android:name=".ui.home.RecommendationCropActivity" />
        <activity android:name=".ui.home.weather.WeatherPredictActivity" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>